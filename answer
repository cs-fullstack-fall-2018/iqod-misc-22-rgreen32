public class iqod {
    public static void main(String[] args){
        int num = 0;

        int[] array1 = {5,3,4};
        int[] array2 = {3,4,6,6};
        int[] array3 = {8,6,3,6,8,4};

        int[][] arrays = {array1, array2, array3};

        for(int[] arr : arrays){
            int first = arr[0];
             num += first;
            System.out.println((first));
        }
        System.out.println(num);



    }
}
